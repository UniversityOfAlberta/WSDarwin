<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="WSDarwin"
            id="wsdarwin_1.0.0">
      </category>
      <view
            name="WSDarwin View"
            icon="icons/sample.gif"
            category="wsdarwin_1.0.0"
            class="wsdarwin.views.WSDarwinView"
            id="wsdarwin.views.WSDarwinView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.ProblemView"
               relationship="right"
               id="wsdarwin.views.WSDarwinView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>
